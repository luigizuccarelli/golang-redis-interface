{
  "project": "Golang Redis Interface",
  "metainfo": "Author LMZ 03/2020",
  "workdir": "work",
  "id": "1002",
  "scm": "git@github.com:luigizuccarelli/golang-redisinterface.git",
  "stages": [
    {
      "id" : 1,
      "name": "Git check",
      "exec": "ls",
      "wait": 5,
      "skip": false,
      "commands": [
        "-la"
      ]
    },
    {
      "id" : 2,
      "name": "Clean",
      "exec": "make",
      "wait": 5,
      "skip": false,
      "commands": [
        "clean"
      ]
    },
    {
      "id" : 3,
      "name": "Test",
      "exec": "make",
      "wait": 5,
      "skip": false,
      "commands": [
        "test"
      ]
    },
    {
      "id" : 4,
      "name": "Cover",
      "exec": "make",
      "wait": 5,
      "skip": false,
      "commands": [
        "cover"
      ]
    },
    {
      "id" : 5,
      "name": "Compile",
      "exec": "make",
      "wait": 10,
      "skip": false,
      "commands": [
        "build"
      ]
    },
    {
      "id" : 6,
      "name": "Container",
      "exec": "podman",
      "wait": 5,
      "skip": true,
      "commands": [
        "build",
        "-t",
        "docker.io/lzuccarelli/golang-redisinterface:1.13.1",
        "."
      ]
    },
    {
      "id" : 7,
      "name": "Deploy",
      "exec": ".microservice",
      "wait": 5,
      "skip": true,
      "replicas" : 3,
      "service": "golang-redisinterface",
      "envars": [
        {
          "name":"SERVER_PORT",
          "value": "8081"
        },
        {
          "name":"LOG_LEVEL",
          "value":"trace"
        },
        {
          "name":"NAME",
          "value":"golang-redisinterface"
        },
        {
          "name":"VERSION",
          "value":"1.0.1"
        }
      ]
    }
  ]
}
